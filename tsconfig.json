{
  "compilerOptions": {
    "target": "es2019",
    "moduleResolution": "node",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "declaration": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
  },
  "exclude": ["node_modules", "dist"],
}